{
  "name": "jam",
  "main": "app/index.js",
  "description": "Jam is a man-in-the-middle proxy server that allows you to easily interact with Animal Jam",
  "author": "sxip",
  "version": "0.0.4",
  "scripts": {
    "start:dev": "npx cross-env NODE_ENV=development nodemon",
    "build:win": "npx electron-builder build -w --x64 --ia32 -p never"
  },
  "devDependencies": {
    "cross-env": "^6.0.3",
    "electron": "^4.0.0",
    "electron-builder": "^21.2.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "nodemon": "^2.0.2"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "electron-root-path": "^1.0.16",
    "electron-tabs": "^0.11.0",
    "express": "latest",
    "hostile": "^1.3.2",
    "jquery": "^3.5.0",
    "lodash": "^4.17.15",
    "os": "^0.1.1",
    "popper.js": "^1.16.0",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "tailwindcss": "^1.2.0"
  },
  "build": {
    "productName": "Jam",
    "appId": "com.jam",
    "icon": "assets/icon.ico",
    "directories": {
      "output": "build"
    },
    "win": {
      "requestedExecutionLevel": "highestAvailable"
    },
    "files": [
      "app/**/*"
    ],
    "extraFiles": [
      "plugins/**/*",
      "assets/**/*",
      "settings.json"
    ]
  }
}
